{
  "name": "sidecar-ts-test",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "engines": {
    "node": ">= 16"
  },
  "dependencies": {
    "brolog": "^1.14.2",
    "frida": "^15.1.17",
    "sidecar": "^1.0.19",
    "state-switch": "^1.6.3"
  },
  "scripts": {
    "start": "npm run start:run",
    "start:dev": "npm run build && npm run start:run",
    "start:run": "cross-env BROLOG_LEVEL=info NODE_OPTIONS=\"--no-warnings --loader=ts-node/esm\" node src/demo/main.ts",
    "dump": "npx sidecar-dump source src/demo/demo-sidecar.ts > sidecar-agent.js",
    "build": "npm run build:namespace",
    "build:base": "frida-compile -t tsconfig-agent.json src/agent/mod.ts -o src/init-agent-script.js",
    "build:namespace": "frida-compile -t tsconfig-agent.json src/agent/mod.ts -o src/init-agent-script.js -s demo",
    "cli": "npm run cli:namespace",
    "cli:base": "npm run build:base && frida node -l src/init-agent-script.js",
    "cli:namespace": "npm run build:namespace && frida node -l src/init-agent-script.js"
  },
  "files": [
    "dist/*",
    "src/",
    "README.md",
    "package.json"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "binsee",
  "license": "ISC",
  "devDependencies": {
    "@chatie/git-scripts": "^0.7.2",
    "@chatie/tsconfig": "^4.6.3",
    "@types/node": "^17.0.23",
    "cross-env": "^7.0.3",
    "frida-compile": "^10.2.5",
    "ts-node": "^10.7.0"
  },
  "git": {
    "scripts": {
      "pre-push": "npx git-scripts-pre-push"
    }
  }
}
